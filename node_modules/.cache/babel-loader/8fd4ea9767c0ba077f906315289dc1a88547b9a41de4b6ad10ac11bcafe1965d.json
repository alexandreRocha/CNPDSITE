{"ast":null,"code":"import ContatoMaps from \"./ContatoMaps.vue\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nimport axios from \"axios\";\nexport default {\n  name: \"ContatoCnpd\",\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n    ContatoMaps\n  },\n  data() {\n    return {\n      nome: \"\",\n      email: \"\",\n      telefone: \"\",\n      assunto: \"\",\n      duvida: \"\",\n      morada: \"\",\n      showModal: false,\n      ErrorModal: false\n    };\n  },\n  methods: {\n    async onSubmit( /*values*/\n    ) {\n      const datas = {\n        nome: this.nome,\n        morada: this.morada,\n        email: this.email,\n        assunto: this.assunto,\n        duvida: this.duvida,\n        telefone: this.telefone\n      };\n\n      //SUBMIT FORM WITH AXIOS\n      try {\n        await axios.post(\"http://127.0.0.1:8000/api/contcmvatos/store\", datas, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data; charset=utf-8\"\n          }\n        });\n        this.showModal = !this.showModal;\n      } catch (error) {\n        this.ErrorModal = !this.ErrorModal;\n        //return alert(\"Error: \" + error);\n      }\n    },\n\n    closeModal() {\n      this.showModal = !this.showModal;\n      window.location.reload();\n    },\n    validateText(value) {\n      // if the field is empty\n      if (!value) {\n        return \"Campo obrigatório!\";\n      }\n      return true;\n    },\n    validateNumber(value) {\n      // if the field is empty\n      if (!value) {\n        return \"Campo obrigatório!\";\n      }\n      // if the field is not a valid number\n      const regex = /[0-9]/;\n      if (!regex.test(value)) {\n        return \"Carater inválido, precisa ser um número!\";\n      }\n      return true;\n    },\n    validateEmail(value) {\n      // if the field is empty\n      if (!value) {\n        return \"Campo obrigatório!\";\n      }\n      // if the field is not a valid email\n      const regex = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,4}$/i;\n      if (!regex.test(value)) {\n        return \"Email inválido, precisa ser nesse formato: cnpd@example.cv\";\n      }\n      // All is good\n      return true;\n    }\n  }\n};","map":{"version":3,"mappings":"AAqNA,OAAOA,WAAU,MAAO,mBAAmB;AAC3C,SAASC,IAAI,EAAEC,KAAK,EAAEC,YAAW,QAAS,cAAc;AAExD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVL,IAAI;IACJC,KAAK;IACLC,YAAY;IACZH;EACF,CAAC;EACDO,IAAI,GAAG;IACL,OAAO;MACHC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,QAAQ,EAAC;IAAA,EAAY;MACzB,MAAMC,KAAI,GAAI;QACZV,IAAI,EAAE,IAAI,CAACA,IAAI;QACfK,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBJ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBE,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBF,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;;MAEA;MACH,IAAI;QACH,MAAMN,KAAK,CAACe,IAAI,CAAC,6CAA6C,EAAED,KAAK,EAAE;UACpEE,OAAO,EAAE;YAAE,cAAc,EAAE;UAAqC;QAClE,CAAC,CAAC;QACF,IAAI,CAACN,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;MAChC,CAAC,QAAMO,KAAK,EAAC;QACX,IAAI,CAACN,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;QAClC;MACF;IAEF,CAAC;;IACDO,UAAU,GAAG;MACX,IAAI,CAACR,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;MAChCS,MAAM,CAACC,QAAQ,CAACC,MAAM,EAAE;IAC1B,CAAC;IACDC,YAAY,CAACC,KAAK,EAAE;MAClB;MACA,IAAI,CAACA,KAAK,EAAE;QACV,OAAO,oBAAoB;MAC7B;MACA,OAAO,IAAI;IACb,CAAC;IAEDC,cAAc,CAACD,KAAK,EAAE;MACpB;MACA,IAAI,CAACA,KAAK,EAAE;QACV,OAAO,oBAAoB;MAC7B;MACA;MACA,MAAME,KAAI,GAAI,OAAO;MACrB,IAAI,CAACA,KAAK,CAACC,IAAI,CAACH,KAAK,CAAC,EAAE;QACtB,OAAO,0CAA0C;MACnD;MAEA,OAAO,IAAI;IACb,CAAC;IACDI,aAAa,CAACJ,KAAK,EAAE;MACnB;MACA,IAAI,CAACA,KAAK,EAAE;QACV,OAAO,oBAAoB;MAC7B;MACA;MACA,MAAME,KAAI,GAAI,2CAA2C;MACzD,IAAI,CAACA,KAAK,CAACC,IAAI,CAACH,KAAK,CAAC,EAAE;QACtB,OAAO,4DAA4D;MACrE;MACA;MACA,OAAO,IAAI;IACb;EACF;AACF,CAAC","names":["ContatoMaps","Form","Field","ErrorMessage","axios","name","components","data","nome","email","telefone","assunto","duvida",