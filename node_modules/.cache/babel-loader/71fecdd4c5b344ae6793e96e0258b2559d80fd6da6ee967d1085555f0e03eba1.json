er-link to=\"#\">\n                <IconAwe class=\"iconnavbarmob\" icon=\"list-check\" />\n                <span id=\"menuiten\">Publicações</span></router-link\n              >\n              <div v-show=\"showpublicacoes\" id=\"showpublicacoes\">\n                <li>\n                  <router-link to=\"/panfletos\"> Panfletos </router-link>\n                </li>\n                <li><router-link to=\"/videos\"> Videos </router-link></li>\n              </div>\n            </li>\n            <li>\n              <router-link to=\"/contato\">\n                <IconAwe class=\"iconnavbarmob\" icon=\"house\" />\n                <span id=\"menuiten\">COntato</span>\n              </router-link>\n            </li>\n          </div>\n        </ul>\n      </div>\n    </header>\n    <!-- End Header  -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"NavBar\",\n  props: [\"logo\", \"alt\"],\n  data() {\n    return {\n      scrolledNav: null,\n      mobile: null,\n      mobileNav: null,\n      windowWidth: null,\n      cnpd: null,\n      showcnpd: null,\n      decisoes: null,\n      showdecisoes: null,\n      canalcidadao: null,\n      showcanalcidadao: null,\n      formulario: null,\n      publicacoes: null,\n      showformulario: null,\n      showpublicacoes: null,\n    };\n  },\n  created() {\n    window.addEventListener(\"resize\", this.checkScreen);\n    this.checkScreen();\n  },\n  methods: {\n    toggleMobileNav() {\n      this.mobileNav = !this.mobileNav;\n    },\n    showcnpditens() {\n      this.showcnpd = !this.showcnpd;\n    },\n    showdecisoesitens() {\n      this.showdecisoes = !this.showdecisoes;\n    },\n    showcanalcidadaoitens() {\n      this.showcanalcidadao = !this.showcanalcidadao;\n    },\n    showformularioitens() {\n      this.showformulario = !this.showformulario;\n    },\n    showpublicacoesitens() {\n      this.showpublicacoes = !this.showpublicacoes;\n    },\n\n    checkScreen() {\n      this.windowWidth = window.innerWidth;\n      if (this.windowWidth <= 750) {\n        this.mobile = true;\n        this.cnpd = true;\n        this.decisoes = true;\n        this.canalcidadao = true;\n        this.formulario = true;\n        this.publicacoes = true;\n        return;\n      }\n      this.mobile = false;\n      this.mobileNav = false;\n      return;\n    },\n  },\n};\n</script>\n\n<style scoped>\n@media screen and (max-width: 600px) {\n  ul {\n    background-color: #0e214b;\n    color: #fff;\n    font-size: 12px;\n    list-style: none;\n  }\n\n  .iconnavbarmob {\n    color: #bd9a13;\n    font-size: 15px;\n    margin: 0 auto;\n  }\n\n  .divmenu {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n  }\n\n  #ulmobile {\n    padding: 25px;\n    color: white;\n  }\n  #showcnpd {\n    padding-left: 0px;\n    background-color: #0e214b;\n    border-radius: 2px;\n    color: white;\n  }\n  #showdecisoes {\n    padding-left: 10px;\n    background-color: #0e214b;\n    border-radius: 2px;\n    color: white;\n  }\n  #showcanalcidadao {\n    padding-left: 10px;\n    background-color: #0e214b;\n    border-radius: 2px;\n    color: white;\n  }\n\n  #showformulario {\n    padding-left: 10px;\n    background-color: #0e214b;\n    border-radius: 2px;\n    color: white;\n  }\n  #showpublicacoes {\n    padding-left: 10px;\n    background-color: #0e214b;\n    border-radius: 2px;\n    color: white;\n  }\n  li {\n    padding: 10px;\n  }\n  #menuiten {\n    margin-left: 25px;\n  }\n  #menuiten1 {\n    margin-left: 20px;\n  }\n}\n.icon-color {\n  cursor: pointer;\n  font-size: 24px;\n  transform: 0.8s ease all;\n  color: white;\n}\n.icon-active {\n  transform: rotate(180deg);\n}\n#header {\n  font-family: \"Times New Roman\", Times, serif;\n  font-size: 48px;\n  background-color: #061536;\n}\n\n.navbar-toggler {\n  background-color: #fff;\n}\n\n/*--------------------------------------------------------------\n# General\n--------------------------------------------------------------*/\nbody {\n  font-family: \"Times New Roman\", Times, serif;\n  color: #444444;\n}\n\na {\n  color: #fff;\n  text-decoration: none;\n}\n\na:hover {\n  color: #bd9a13;\n  text-decoration: none;\n}\n\n/*--------------------------------------------------------------\n# Header\n--------------------------------------------------------------*/\n#header {\n  transition: all 0.5s;\n  z-index: 997;\n  padding: 20px 0;\n}\n#header .logo {\n  font-size: 30px;\n  margin: 0;\n  padding: 0;\n  line-height: 1;\n  font-weight: 600;\n  letter-spacing: 2px;\n  text-transform: uppercase;\n}\n#header .logo a {\n  color: #fff;\n}\n#header .logo img {\n  max-height: 45px;\n}\n#header.header-scrolled,\n#header.header-inner-pages {\n  background: rgba(0, 0, 0, 0.9);\n  padding: 12px 0;\n}\n\n#img {\n  width: 100%;\n  height: 10px;\n}\n/*--------------------------------------------------------------\n# Navigation Menu\n--------------------------------------------------------------*/\n/**\n* Desktop Navigation \n*/\n.navbar {\n  padding: 0;\n}\n.navbar ul {\n  margin: 0;\n  padding: 0;\n  display: flex;\n  list-style: none;\n  align-items: center;\n}\n.navbar li {\n  position: relative;\n}\n.navbar > ul > li {\n  white-space: nowrap;\n  padding: 10px 0 10px 24px;\n}\n.navbar a,\n.navbar a:focus {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  font-family: \"Times New Roman\", Times, serif; \n  color: #fff;\n  font-weight: bold;\n  font-size: 16px;\n  padding: 0 4px;\n  white-space: nowrap;\n  transition: 0.3s;\n  letter-spacing: 0.4px;\n  position: relative;\n}\n.navbar a i,\n.navbar a:focus i {\n  font-size: 12px;\n  line-height: 0;\n  margin-left: 5px;\n}\n.navbar > ul > li > a:before {\n  content: \"\";\n  position: absolute;\n  width: 100%;\n  height: 2px;\n  bottom: -6px;\n  left: 0;\n  background-color: #bd9a13;\n  visibility: hidden;\n  transform: scaleX(0);\n  transition: all 0.3s ease-in-out 0s;\n}\n.navbar a:hover:before,\n.navbar li:hover > a:before,\n.navbar .active:before {\n  visibility: visible;\n  transform: scaleX(1);\n}\n.navbar a:hover,\n.navbar .active,\n.navbar .active:focus,\n.navbar li:hover > a {\n  color: #cfac0b;\n}\n.navbar .dropdown ul {\n  display: block;\n  position: absolute;\n  left: 24px;\n  top: calc(100% + 30px);\n  margin: 0;\n  padding: 10px 0;\n  z-index: 99;\n  opacity: 0;\n  visibility: hidden;\n  background: #013370;\n  color: #fff;\n  border-radius: 10px;\n  box-shadow: 0px 0px 10px #cfac0b; /*cor de sombra de dropdown ul*/\n  transition: 0.3s;\n}\n.navbar .dropdown ul li {\n  min-width: 200px;\n}\n.navbar .dropdown ul a {\n  padding: 10px 20px;\n  font-size: 14px;\n  text-transform: none;\n  color: #fff; /*COR DE LI NO UL DROPDOWN */\n}\n.navbar .dropdown ul a i {\n  font-size: 12px;\n}\n.navbar .dropdown ul a:hover,\n.navbar .dropdown ul .active:hover,\n.navbar .dropdown ul li:hover > a {\n  color: #bd9a13;\n}\n.navbar .dropdown:hover > ul {\n  opacity: 1;\n  top: 100%;\n  visibility: visible;\n}\n.navbar .dropdown .dropdown ul {\n  top: 0;\n  left: calc(100% - 30px);\n  visibility: hidden;\n}\n.navbar .dropdown .dropdown:hover > ul {\n  opacity: 1;\n  top: 0;\n  left: 100%;\n  visibility: visible;\n}\n@media (max-width: 1024px) {\n  .navbar .dropdown .dropdown ul {\n    left: -90%;\n  }\n  .navbar .dropdown .dropdown:hover > ul {\n    left: -100%;\n  }\n}\n\n/**\n* Mobile Navigation \n*/\n\n@media screen and (min-width: 600px) and (max-width: 2024px) {\n  .icon-color {\n    display: none;\n    color: #fff;\n  } /* show it on small screens */\n}\n\n@media screen and (min-width: 0px) and (max-width: 600px) {\n  .icon-color {\n    display: block;\n  } /* hide it elsewhere */\n}\n\n.mobile-nav-toggle {\n  color: #fff;\n  font-size: 28px;\n  cursor: pointer;\n  display: none;\n  line-height: 0;\n  transition: 0.5s;\n}\n\n@media (max-width: 600px) {\n  .mobile-nav-toggle {\n    display: block;\n  }\n\n  .navbar ul {\n    display: none;\n  }\n}\n.navbar-mobile {\n  position: fixed;\n  overf