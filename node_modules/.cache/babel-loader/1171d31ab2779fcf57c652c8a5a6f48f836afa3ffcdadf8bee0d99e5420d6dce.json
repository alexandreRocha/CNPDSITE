n true;\n      });\n    }\n  },\n  created() {\n    axios.get('https://jsonplaceholder.typicode.com/users').then(response => {\n      this.listas = response.data;\n    });\n  },\n  mounted() {\n    document.title = \"Autorizações | CNPD\";\n  }\n};","map":{"version":3,"mappings":"AAgEA;AACA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAC,kBAAkB;EACvBC,IAAI,EAAE,OAAO;IACbC,MAAM,EAAE,EAAE;IAAC;IACXC,WAAW,EAAC,MAAM;IAClBC,cAAc,EAAC,KAAK;IACpBC,MAAM,EAAE,EAAE;IACVC,eAAe,EAAE,EAAE;IACnBC,QAAQ,EAAE,CAAC;IACXC,WAAW,EAAE;EACf,CAAC,CAAC;EAEFC,OAAO,EAAC;IACNC,IAAI,CAACC,CAAC,EAAE;MACN,IAAGA,MAAM,IAAI,CAACR,WAAW,EAAE;QACzB,IAAI,CAACC,cAAa,GAAI,IAAI,CAACA,cAAc,KAAG,KAAK,GAAC,MAAM,GAAC,KAAK;MAChE;MACA,IAAI,CAACD,WAAU,GAAIQ,CAAC;IACtB,CAAC;IACDC,QAAQ,GAAG;MACT,IAAI,IAAI,CAACJ,WAAW,GAAC,IAAI,CAACD,QAAQ,GAAI,IAAI,CAACL,MAAM,CAACW,MAAM,EAAE,IAAI,CAACL,WAAW,EAAE;IAC9E,CAAC;IACDM,QAAQ,GAAG;MACT,IAAG,IAAI,CAACN,WAAU,GAAI,CAAC,EAAE,IAAI,CAACA,WAAW,EAAE;IAC7C,CAAC;IAEDO,YAAY,GAAE;MACZ,OAAO,IAAI,CAACb,MAAM,CAACW,MAAM;IAC3B;EACF,CAAC;EAEDG,QAAQ,EAAE;IACRC,cAAc,GAAG;MACf,OAAO,IAAI,CAACf,MAAM,CAACQ,IAAI,CAAC,CAACQ,CAAC,EAACC,CAAC,KAAK;QAC/B,IAAIC,QAAO,GAAI,CAAC;QAChB,IAAG,IAAI,CAAChB,cAAa,KAAM,MAAM,EAAEgB,QAAO,GAAI,CAAC,CAAC;QAChD,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,CAAC,IAAIiB,QAAQ;QAClE,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,IAAIiB,QAAQ;QACjE,OAAO,CAAC;MACV,CAAC,CAAC,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QACxB,IAAIC,KAAI,GAAI,CAAC,IAAI,CAAChB,WAAW,GAAC,CAAC,IAAE,IAAI,CAACD,QAAQ;QAC9C,IAAIkB,GAAE,GAAI,IAAI,CAACjB,WAAW,GAAC,IAAI,CAACD,QAAQ;QACxC,IAAGgB,KAAI,IAAKC,KAAI,IAAKD,KAAI,GAAIE,GAAG,EAAE,OAAO,IAAI;MAC/C,CAAC,CAAC;IACJ,CAAC;IACDC,YAAW,GAAK;MACd,OAAO,IAAI,CAACxB,MAAM,CAACmB,MAAM,CAAEpB,IAAI,IAAK;QAClC;QACA,IAAID,IAAG,GAAIC,IAAI,CAACD,IAAI,CAAC2B,WAAW,EAAE,CAACC,KAAK,CAAC,IAAI,CAACvB,MAAM,CAACsB,WAAW,EAAE,CAAC;QACnE;QACA,IAAIE,KAAI,GAAI5B,IAAI,CAAC4B,KAAK,CAACF,WAAW,EAAE,CAACC,KAAK,CAAC,IAAI,CAACv