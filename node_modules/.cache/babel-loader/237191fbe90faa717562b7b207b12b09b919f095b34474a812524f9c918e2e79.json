{"ast":null,"code":"/*eslint-disable*/\nimport axios from 'axios';\nexport default {\n  name: 'ComunicadosCnpd',\n  data: () => ({\n    listas: [],\n    //meu array com os itens que vem do API\n    currentSort: 'name',\n    currentSortDir: 'asc',\n    search: '',\n    searchSelection: '',\n    pageSize: 8,\n    currentPage: 1\n  }),\n  methods: {\n    sort(s) {\n      if (s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n      }\n      this.currentSort = s;\n    },\n    nextPage() {\n      if (this.currentPage * this.pageSize < this.listas.length) this.currentPage++;\n    },\n    prevPage() {\n      if (this.currentPage > 1) this.currentPage--;\n    },\n    countNumbers() {\n      return this.listas.length;\n    }\n  },\n  computed: {\n    sortedActivity() {\n      return this.listas.sort((a, b) => {\n        let modifier = 1;\n        if (this.currentSortDir === 'desc') modifier = -1;\n        if (a[this.currentSort] < b[this.currentSort]) return -1 * modifier;\n        if (a[this.currentSort] > b[this.currentSort]) return 1 * modifier;\n        return 0;\n      }).filter((row, index) => {\n        let start = (this.currentPage - 1) * this.pageSize;\n        let end = this.currentPage * this.pageSize;\n        if (index >= start && index < end) return true;\n      });\n    },\n    filteredList() {\n      return this.listas.filter(data => {\n        let titulo = data.titulo.toLowerCase().match(this.search.toLowerCase());\n        let num_doc = data.num_doc.toLowerCase().match(this.search.toLowerCase());\n        return titulo || num_doc;\n      }).filter((row, index) => {\n        let start = (this.currentPage - 1) * this.pageSize;\n        let end = this.currentPage * this.pageSize;\n        if (index >= start && index < end) return true;\n      });\n    }\n  },\n  created() {\n    axios.get('http://127.0.0.1:8000/api/comunicados').then(response => {\n      this.listas = response.data;\n    });\n  },\n  mounted() {\n    window.scrollTo(0, 0); // Rola para o topo da página quando a página é carregada\n  }\n};","map":{"version":3,"names":["axios","name","data","listas","currentSort","currentSortDir","search","searchSelection","pageSize","currentPage","methods","sort","s","nextPage","length","prevPage","countNumbers","computed","sortedActivity","a","b","modifier","filter","row","index","start","end","filteredList","titulo","toLowerCase","match","num_doc","created","get","then","response","mounted","window","scrollTo"],"sources":["/Users/jailsoncorreia/CNPDSYSTEM/SITECNPDFINAL/src/components/ComunicadosCnpd.vue"],"sourcesContent":["<template>\n  <section id=\"comunicados\" class=\"comunicados\">\n    <div class=\"container\">\n      <div class=\"section-bredcumbs\">\n        <h5>\n          Onde estou?\n          <router-link style=\"text-decoration: none\" to=\"/\">\n            <b>Página Inicial</b>\n          </router-link>\n          <b> > </b>\n          <router-link style=\"text-decoration: none\" to=\"#\">\n            <b>Relações Públicas</b>\n          </router-link>\n          <b> > </b>\n          <router-link style=\"text-decoration: none\" to=\"/diretivas\">\n            <b>Comunicados Publicadas</b>\n          </router-link>\n        </h5>\n      </div>\n      <div class=\"section-title\">\n        <h2>Comunicados da CNPD</h2>\n      </div>\n\n      <div class=\"col-md-12\">\n        <div class=\"form-group\">\n          <p class=\"pleft\">Comunicados disponíveis: {{ countNumbers() }}</p>\n          <input\n            id=\"idsearch\"\n            type=\"text\"\n            class=\"form-control\"\n            v-model=\"search\"\n            placeholder=\"Procurar por título ou por ano ...\"\n          />\n        </div>\n        <br>\n\n        <div\n          id=\"box\"\n          v-for=\"(pub, index) in filteredList\"\n          :key=\"index\"\n          class=\"row\"\n        >\n          <router-link :to=\"{ name: 'comunicadoid', params: { id: pub.id } }\">\n            <div class=\"post-box\" alt=\"Abrir Isenção em PDF\">\n              <!-- :src=\"pub.url\" src=\"/img/lei.png\"-->\n\n              <div id=\"comunicado\" class=\"first\">\n                <img src=\"/img/press.png\" alt=\"Avatar\" style=\"width: 60px\" />\n              </div>\n              <div id=\"comunicado\" class=\"second\">\n                <h5 class=\"post-title\">\n                  {{ pub.titulo }}\n                </h5>\n              </div>\n              <div id=\"comunicado\" class=\"third\">\n                <div class=\"icon\">\n                  <IconAwe class=\"icon-color\" icon=\"file-pdf\" />\n                </div>\n              </div>\n            </div>\n          </router-link>\n        </div>\n        <div id=\"idpage\">\n          <button\n            id=\"button\"\n            @click=\"prevPage\"\n            class=\"float-left btn btn-outline-info btn-sm\"\n          >\n            <i class=\"fas fa-arrow-left\"></i>\n          </button>\n          <button\n            id=\"button\"\n            @click=\"nextPage\"\n            class=\"float-right btn btn-outline-info btn-sm\"\n          >\n            <i class=\"fas fa-arrow-right\"></i>\n          </button>\n        </div>    \n      </div>\n\n\n    </div>\n             \n </section> \n</template>\n\n<script>\n/*eslint-disable*/\nimport axios from 'axios';\n\nexport default { \n    name:'ComunicadosCnpd',\n    data: () => ({\n    listas: [],//meu array com os itens que vem do API\n    currentSort:'name',\n    currentSortDir:'asc',\n    search: '',\n    searchSelection: '',\n    pageSize: 8,\n    currentPage: 1\n  }),\n\n  methods:{\n    sort(s) {\n      if(s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir==='asc'?'desc':'asc';\n      }\n      this.currentSort = s;\n    },\n    nextPage() {\n      if((this.currentPage*this.pageSize) < this.listas.length) this.currentPage++;\n    },\n    prevPage() {\n      if(this.currentPage > 1) this.currentPage--;\n    },\n\n    countNumbers(){\n      return this.listas.length; \n    }\n  },\n\n  computed: { \n    sortedActivity() {\n      return this.listas.sort((a,b) => {\n        let modifier = 1;\n        if(this.currentSortDir === 'desc') modifier = -1;\n        if(a[this.currentSort] < b[this.currentSort]) return -1 * modifier;\n        if(a[this.currentSort] > b[this.currentSort]) return 1 * modifier;\n        return 0;\n      }).filter((row, index) => {\n        let start = (this.currentPage-1)*this.pageSize;\n        let end = this.currentPage*this.pageSize;\n        if(index >= start && index < end) return true;\n      });\n    },\n    filteredList () {\n      return this.listas.filter((data) => { \n        let titulo = data.titulo.toLowerCase().match(this.search.toLowerCase()); \n        let num_doc = data.num_doc.toLowerCase().match(this.search.toLowerCase());\n        return titulo || num_doc;\n      }).filter((row, index) => {\n        let start = (this.currentPage-1)*this.pageSize;\n        let end = this.currentPage*this.pageSize;\n        if(index >= start && index < end) return true;\n      });\n    }\n  },\n\n  created () {\n    axios.get('http://127.0.0.1:8000/api/comunicados')\n      .then(response => {\n        this.listas = response.data  \n      },)\n  }, \n  mounted() {\n    window.scrollTo(0, 0); // Rola para o topo da página quando a página é carregada\n  },\n\n}\n</script>\n\n<style>\n#idpage {\n  display: flex;\n  justify-content: center;\n}\n#button{\n   \n  color: #061536;\n  border: 2px solid #061536;\n  box-shadow: 1px 1px #061536;\n  height: 30px;\n  width: 30px; \n}\n#button:hover{\n  box-shadow: 1px 1px  3px 3px black;\n  color: #fff;\n  background-color: #061536;\n}\n#idsearch{\n  width: 40%;\n  height: 30px;\n  text-align: center;\n  box-shadow: 1px 1px  #061536;\n} \n\n.pcente{\n    text-align: center;\n    font-weight: bold;\n    margin-left: 10%;\n    margin-right:  10%;\n}\n.pcenter{\n    text-align: center; \n    margin-left: 20%;\n    margin-right:  20%;\n    font-weight: bold\n}\n.pleft{\n    text-align: left;  \n    font-weight: bold\n}\n \n.section-title {\n  text-align: center;\n  padding-bottom: 30px;\n}\n.section-title h2 {\n  font-size: 13px;\n  letter-spacing: 1px;\n  font-weight: 700;\n  padding: 8px 20px;\n  line-height: 1;\n  margin: 0;\n  background: #BD9A13;\n  color: #000;\n  display: inline-block;\n  text-transform: uppercase;\n  border-radius: 50px;\n}  \n \n \n.comunicados {\n  background: #fff; \n  padding-top: 120px;\n   font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50; \n} \na{\n    text-decoration: none;\n}\n.comunicados .icon-box {\n  display: flex;\n  align-items: center;\n  padding: 20px;\n  transition: ease-in-out 0.3s;\n  box-shadow: 2px 0 35px 0 rgba(68, 88, 144, 0.12);\n}\n.comunicados .icon-box i {\n  font-size: 32px;\n  padding-right: 10px;\n  line-height: 1;\n}\n.comunicados .icon-box h3 {\n  font-weight: 700;\n  margin: 0;\n  padding: 0;\n  line-height: 1;\n  font-size: 16px;\n}\n.comunicados .icon-box h3 a {\n  color: #493c3e;\n  transition: ease-in-out 0.3s;\n}\n.comunicados .icon-box:hover a {\n  color: #BD9A13; \n}\n\nth {\n  cursor:pointer;\n  /* width: 500px !important; */\n  white-space: nowrap;\n}\n\ntr {\n  white-space: nowrap;\n}\n.table-responsive{\n    text-align: left;\n}\n\n.trcabecalho{\n    font-weight: bold;\n    background-color: #fff; \n}\n \n.post-box {\n  box-shadow: 0px 0 30px rgba(1, 41, 112, 0.08);\n  transition: 0.3s;\n  overflow: hidden;\n  padding: 10px;\n  border-radius: 8px;\n  margin-bottom: 10px;\n}\n/*\nimg {\n  border-radius: 80%;\n}*/\n#comunicado {\n  margin: 10px;\n}\n\n.first {\n  width: 5%;\n  display: inline-block;\n}\n\n.second {\n  width: 75%;\n  display: inline-block;\n}\n\n.third {\n  width: 5%;\n  display: inline-block;\n}\n\n@media screen and (max-width: 500px) {\n  .first,\n  .second,\n  .third {\n    width: 100%;\n  }\n}\n \n.post-box .post-title {\n  font-size: 16px;\n  color: #374253;\n  margin-bottom: 18px;\n  position: relative;\n  text-align: left;\n  transition: 0.3s;\n}\n@media (min-width: 1024px) {\n  .section-title p {\n    width: 50%;\n  }\n}\n\n.icon .icon-color {\n  font-size: 35px;\n  line-height: 1;\n  color: #013365;\n  transition: all 0.9s ease-in-out;\n}\n  \n.form-group {\n  display: flex;\n  justify-content: space-between;\n}\n</style>\n"],"mappings":"AAuFA;AACA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAC,iBAAiB;EACtBC,IAAI,EAAEA,CAAA,MAAO;IACbC,MAAM,EAAE,EAAE;IAAC;IACXC,WAAW,EAAC,MAAM;IAClBC,cAAc,EAAC,KAAK;IACpBC,MAAM,EAAE,EAAE;IACVC,eAAe,EAAE,EAAE;IACnBC,QAAQ,EAAE,CAAC;IACXC,WAAW,EAAE;EACf,CAAC,CAAC;EAEFC,OAAO,EAAC;IACNC,IAAIA,CAACC,CAAC,EAAE;MACN,IAAGA,CAAA,KAAM,IAAI,CAACR,WAAW,EAAE;QACzB,IAAI,CAACC,cAAa,GAAI,IAAI,CAACA,cAAc,KAAG,KAAK,GAAC,MAAM,GAAC,KAAK;MAChE;MACA,IAAI,CAACD,WAAU,GAAIQ,CAAC;IACtB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACJ,WAAW,GAAC,IAAI,CAACD,QAAQ,GAAI,IAAI,CAACL,MAAM,CAACW,MAAM,EAAE,IAAI,CAACL,WAAW,EAAE;IAC9E,CAAC;IACDM,QAAQA,CAAA,EAAG;MACT,IAAG,IAAI,CAACN,WAAU,GAAI,CAAC,EAAE,IAAI,CAACA,WAAW,EAAE;IAC7C,CAAC;IAEDO,YAAYA,CAAA,EAAE;MACZ,OAAO,IAAI,CAACb,MAAM,CAACW,MAAM;IAC3B;EACF,CAAC;EAEDG,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACf,MAAM,CAACQ,IAAI,CAAC,CAACQ,CAAC,EAACC,CAAC,KAAK;QAC/B,IAAIC,QAAO,GAAI,CAAC;QAChB,IAAG,IAAI,CAAChB,cAAa,KAAM,MAAM,EAAEgB,QAAO,GAAI,CAAC,CAAC;QAChD,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,CAAC,IAAIiB,QAAQ;QAClE,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,IAAIiB,QAAQ;QACjE,OAAO,CAAC;MACV,CAAC,CAAC,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QACxB,IAAIC,KAAI,GAAI,CAAC,IAAI,CAAChB,WAAW,GAAC,CAAC,IAAE,IAAI,CAACD,QAAQ;QAC9C,IAAIkB,GAAE,GAAI,IAAI,CAACjB,WAAW,GAAC,IAAI,CAACD,QAAQ;QACxC,IAAGgB,KAAI,IAAKC,KAAI,IAAKD,KAAI,GAAIE,GAAG,EAAE,OAAO,IAAI;MAC/C,CAAC,CAAC;IACJ,CAAC;IACDC,YAAWA,CAAA,EAAK;MACd,O