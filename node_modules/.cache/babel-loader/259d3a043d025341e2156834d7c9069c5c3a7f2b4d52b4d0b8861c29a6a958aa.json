{"ast":null,"code":"/*eslint-disable*/\nimport axios from 'axios';\nexport default {\n  name: 'AutorizacoesCnpd',\n  data: () => ({\n    listas: [],\n    //meu array com os itens que vem do API\n    currentSort: 'entidade',\n    currentSortDir: 'asc',\n    search: '',\n    searchSelection: '',\n    pageSize: 5,\n    currentPage: 1\n  }),\n  methods: {\n    sort(s) {\n      if (s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n      }\n      this.currentSort = s;\n    },\n    nextPage() {\n      if (this.currentPage * this.pageSize < this.listas.length) this.currentPage++;\n    },\n    prevPage() {\n      if (this.currentPage > 1) this.currentPage--;\n    },\n    countNumbers() {\n      return this.listas.length;\n    }\n  },\n  computed: {\n    sortedActivity() {\n      return this.listas.sort((a, b) => {\n        let modifier = 1;\n        if (this.currentSortDir === 'desc') modifier = -1;\n        if (a[this.currentSort] < b[this.currentSort]) return -1 * modifier;\n        if (a[this.currentSort] > b[this.currentSort]) return 1 * modifier;\n        return 0;\n      }).filter((row, index) => {\n        let start = (this.currentPage - 1) * this.pageSize;\n        let end = this.currentPage * this.pageSize;\n        if (index >= start && index < end) return true;\n      });\n    },\n    filteredList() {\n      return this.listas.filter(data => {\n        //let email = data.email.toLowerCase().match(this.search.toLowerCase());\n        let entidade = data.entidade.toLowerCase().match(this.search.toLowerCase());\n        let tipo_entidade = data.tipo_entidade.toLowerCase().match(this.search.toLowerCase());\n        let num_decisao = data.num_decisao.toLowerCase().match(this.search.toLowerCase());\n        return entidade || num_decisao || tipo_entidade /* || city || num_decisao*/;\n      }).filter((row, index) => {\n        let start = (this.currentPage - 1) * this.pageSize;\n        let end = this.currentPage * this.pageSize;\n        if (index >= start && index < end) return true;\n      });\n    }\n  },\n  created() {\n    axios.get('http://127.0.0.1:8000/api/autorizacoes').then(response => {\n      this.listas = response.data;\n    });\n  },\n  mounted() {\n    document.title = \"Autorizações | CNPD\";\n    window.scrollTo(0, 0); // Rola para o topo da página quando a página é carregada\n  }\n};","map":{"version":3,"mappings":"AAgDA;AACA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAC,kBAAkB;EACvBC,IAAI,EAAE,OAAO;IACbC,MAAM,EAAE,EAAE;IAAC;IACXC,WAAW,EAAC,UAAU;IACtBC,cAAc,EAAC,KAAK;IACpBC,MAAM,EAAE,EAAE;IACVC,eAAe,EAAE,EAAE;IACnBC,QAAQ,EAAE,CAAC;IACXC,WAAW,EAAE;EACf,CAAC,CAAC;EAEFC,OAAO,EAAC;IACNC,IAAI,CAACC,CAAC,EAAE;MACN,IAAGA,MAAM,IAAI,CAACR,WAAW,EAAE;QACzB,IAAI,CAACC,cAAa,GAAI,IAAI,CAACA,cAAc,KAAG,KAAK,GAAC,MAAM,GAAC,KAAK;MAChE;MACA,IAAI,CAACD,WAAU,GAAIQ,CAAC;IACtB,CAAC;IACDC,QAAQ,GAAG;MACT,IAAI,IAAI,CAACJ,WAAW,GAAC,IAAI,CAACD,QAAQ,GAAI,IAAI,CAACL,MAAM,CAACW,MAAM,EAAE,IAAI,CAACL,WAAW,EAAE;IAC9E,CAAC;IACDM,QAAQ,GAAG;MACT,IAAG,IAAI,CAACN,WAAU,GAAI,CAAC,EAAE,IAAI,CAACA,WAAW,EAAE;IAC7C,CAAC;IAEDO,YAAY,GAAE;MACZ,OAAO,IAAI,CAACb,MAAM,CAACW,MAAM;IAC3B;EACF,CAAC;EAEDG,QAAQ,EAAE;IACRC,cAAc,GAAG;MACf,OAAO,IAAI,CAACf,MAAM,CAACQ,IAAI,CAAC,CAACQ,CAAC,EAACC,CAAC,KAAK;QAC/B,IAAIC,QAAO,GAAI,CAAC;QAChB,IAAG,IAAI,CAAChB,cAAa,KAAM,MAAM,EAAEgB,QAAO,GAAI,CAAC,CAAC;QAChD,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,CAAC,IAAIiB,QAAQ;QAClE,IAAGF,CAAC,CAAC,IAAI,CAACf,WAAW,IAAIgB,CAAC,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE,OAAO,IAAIiB,QAAQ;QACjE,OAAO,CAAC;MACV,CAAC,CAAC,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QACxB,IAAIC,KAAI,GAAI,CAAC,IAAI,CAAChB,WAAW,GAAC,CAAC,IAAE,IAAI,CAACD,QAAQ;QAC9C,IAAIkB,GAAE,GAAI,IAAI,CAACjB,WAAW,GAAC,IAAI,CAACD,QAAQ;QACxC,IAAGgB,KAAI,IAAKC,KAAI,IAAKD,KAAI,GAAIE,GAAG,EAAE,OAAO,IAAI;MAC/C,CAAC,CAAC;IACJ,CAAC;IACDC,YAAW,GAAK;MACd,OAAO,IAAI,CAACxB,MAAM,CAACmB,MAAM,CAAEpB,IAAI,IAAK;QAClC;QACA,IAAI0B,QAAO,GAAI1B,IAAI,CAAC0B,QAAQ,CAACC,WAAW,EAAE,CAACC,KAAK,CAAC,IAAI,CAACxB,MAAM,CAACuB,WAAW,EAAE,CAAC;QAC3E,IAAIE,aAAY,GAAI7B,IAAI,CAAC6B,aAAa,CAACF,WAAW,EAAE,CAACC,KAAK,CAAC,IAAI,CAACxB,MAAM,CAACuB,WAAW,EAAE,CAAC;QACrF,IAAIG,WAAU,GAAI9B,IAAI,CAAC8B,WAAW,CAACH,WAAW,EAAE,CAACC,KAAK,CAAC,IAAI,CAACxB,MAAM,CAACuB,WAAW,EAAE,CAAC;QACjF,OAAOD,QAAO,IAAKI,WAAU,IAAKD,aAAa;MACjD,CAAC,CAAC,CAACT,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QACxB,IAAIC,KAAI,GAAI,CAAC,IAAI,CAAChB,WAAW,GAAC,CAAC,IAAE,IAAI,CAACD,QAAQ;QAC9C,IAAIkB,GAAE,GAAI,IAAI,CAACjB,WAAW,GAAC,IAAI,CAACD,QAAQ;QACxC,IAAGgB,KAAI,IAAKC,KAAI,IAAKD,KAAI,GAAIE,GAAG,EAAE,OAAO,IAAI;MAC/C,CAAC,CAAC;IACJ;EACF,CAAC;EAEDO,OAAM,GAAK;IACTjC,KAAK,CAACkC,GAAG,CAAC,wCAAwC,EAC/CC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACjC,MAAK,GAAIiC,QAAQ,CAAClC,IAAG;IAC5B,CAAC,CAAC;EACN,CAAC;EACDmC,OAAO,GAAG;IACRC,QAAQ,CAACC,KAAI,GAAI,qBAAqB;IACtCC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE;EACzB;AAEF","names":["axios","name","data","listas","currentSort","currentSortDir","search","searchSelection","pageSize","currentPage","methods","sort","s","nextPage","length","prevPage","countNumbers","computed","sortedActivity","a","b","modifier","filter","row","index","start","end","filteredList","entidade","toLowerCase","match","tipo_entidade","num_decisao","created","get","then","response","mounted","document","title","window","scrollTo"],"sourceRoot":"","sources":["/Users/jailsoncorreia/CNPDSYSTEM/SITECNPDFINAL/src/components/AutorizacoesCnpd.vue"],"sourcesContent":["<template>\n <section id=\"decisoes\" class=\"decisoes\">\n    <div class=\"container\">\n      <div class=\"section-bredcumbs\">\n        <h5>\n          Onde estou?\n          <router-link style=\"text-decoration: none\" to=\"/\">\n            <b>Página Inicial</b>\n          </router-link>\n          <b> > </b>\n          <router-link style=\"text-decoration: none\" to=\"#\">\n            <b>Decisões</b>\n          </router-link>\n          <b> > </b>\n          <router-link style=\"text-decoration: none\" to=\"/autorizacoes\">\n            <b>Autorizações Publicadas</b>\n          </router-link>\n        </h5>\n      </div>\n        <div class=\"section-title\">\n            <h2>Decisões da CNPD</h2> \n        </div> \n          <div class=\"col-md-12\">\n            <div class=\"form-group\">\n              <p class=\"pleft\">Autorizações disponíveis: {{countNumbers()}}</p> \n              <input id=\"idsearch\" type=\"text\" class=\"form-control\" v-model=\"search\" placeholder=\"Procurar por Entidades ou Nº decisão ...\">\n            </div> \n            <br>\n            <div v-for=\"(lista) in (sortedActivity, filteredList)\" :key=\"lista.id\"> \n              <div id=\"autos\">\n                <router-link id=\"links\" :to=\"{name: 'autoview',params: { id: lista.id}}\">  \n                  <div class=\"alert alert-secondary\" role=\"alert\">\n                    <b id=\"title\">{{ lista.tipo_processo === 'Autorizacao' ? 'Autorização' : '' }} Nº.{{ lista.num_decisao }} - {{ lista.entidade }}</b> \n                    <br><b id=\"date\">Data Decisão: {{lista.data_decisao}}</b>\n                  </div>\n                </router-link> \n              </div> \n            </div>\n            <div id=\"idpage\">\n              <button id=\"button\" @click=\"prevPage\" class=\"float-left btn btn-outline-info btn-sm\"><i class=\"fas fa-arrow-left\"></i> </button> \n              <button id=\"button\" @click=\"nextPage\" class=\"float-right btn btn-outline-info btn-sm\"> <i class=\"fas fa-arrow-right\"></i></button>\n            </div>\n          </div> \n    </div>       \n </section> \n</template>\n\n<script>\n/*eslint-disable*/\nimport axios from 'axios';\n\nexport default { \n    name:'AutorizacoesCnpd',\n    data: () => ({\n    listas: [],//meu array com os itens que vem do API\n    currentSort:'entidade',\n    currentSortDir:'asc',\n    search: '',\n    searchSelection: '',\n    pageSize: 5,\n    currentPage: 1,\n  }),\n\n  methods:{\n    sort(s) {\n      if(s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir==='asc'?'desc':'asc';\n      }\n      this.currentSort = s;\n    },\n    nextPage() {\n      if((this.currentPage*this.pageSize) < this.listas.length) this.currentPage++;\n    },\n    prevPage() {\n      if(this.currentPage > 1) this.currentPage--;\n    },\n\n    countNumbers(){\n      return this.listas.length; \n    }\n  },\n\n  computed: { \n    sortedActivity() {\n      return this.listas.sort((a,b) => {\n        let modifier = 1;\n        if(this.currentSortDir === 'desc') modifier = -1;\n        if(a[this.currentSort] < b[this.currentSort]) return -1 * modifier;\n        if(a[this.currentSort] > b[this.currentSort]) return 1 * modifier;\n        return 0;\n      }).filter((row, index) => {\n        let start = (this.currentPage-1)*this.pageSize;\n        let end = this.currentPage*this.pageSize;\n        if(index >= start && index < end) return true;\n      });\n    },\n    filteredList () {\n      return this.listas.filter((data) => {\n        //let email = data.email.toLowerCase().match(this.search.toLowerCase());\n        let entidade = data.entidade.toLowerCase().match(this.search.toLowerCase());\n        let tipo_entidade = data.tipo_entidade.toLowerCase().match(this.search.toLowerCase());\n        let num_decisao = data.num_decisao.toLowerCase().match(this.search.toLowerCase());\n        return entidade || num_decisao || tipo_entidade/* || city || num_decisao*/;\n      }).filter((row, index) => {\n        let start = (this.currentPage-1)*this.pageSize;\n        let end = this.currentPage*this.pageSize;\n        if(index >= start && index < end) return true;\n      });\n    }\n  },\n\n  created () {\n    axios.get('http://127.0.0.1:8000/api/autorizacoes')\n      .then(response => {\n        this.listas = response.data  \n      },)\n  },\n  mounted() {  \n    document.title = \"Autorizações | CNPD\";  \n    window.scrollTo(0, 0); // Rola para o topo da página quando a página é carregada\n  }, \n\n}\n</script>\n\n<style>\n#title{ \n  font-size: 14px;\n}\n#date{ \nfont-size: 10px;\n}\n#idpage {\n  display: flex;\n  justify-content: center;\n}\n#button{\n   \n  color: #061536;\n  border: 2px solid #061536;\n  box-shadow: 1px 1px #061536;\n  height: 30px;\n  width: 30px; \n}\n#button:hover{\n  box-shadow: 1px 1px  3px 3px black;\n  color: #fff;\n  background-color: #061536;\n}\n \n.form-group{\n  display: flex;\n  justify-content: space-between;\n}\n\n \n.pleft{\n    text-align: left;  \n    font-weight: bold\n}\n \n.section-title {\n  text-align: center;\n  padding-bottom: 30px;\n}\n.section-title h2 {\n  font-size: 13px;\n  letter-spacing: 1px;\n  font-weight: 700;\n  padding: 8px 20px;\n  line-height: 1;\n  margin: 0;\n  background: #BD9A13;\n  color: #000;\n  display: inline-block;\n  text-transform: uppercase;\n  border-radius: 50px;\n}  \n \n \n.decisoes {\n  background: #fff; \n  padding-top: 120px;\n  font-family: Arial, Helvetica, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50; \n} \na{\n    text-decoration: none;\n}\n  \n#title{ \n  font-size: 14px;\n}\n#date{ \nfont-size: 10px;\n}\n#autos{\n  text-align: left;\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;  \n} \n#autos:hover{ \n  border-width: 2px; /* Aumenta a largura da borda ao passar o mouse */\n  box-shadow: 0px 0px 10px #2c3e50;\n  border-radius: 10px;\n}\n.trcabecalho{\n    font-weight: bold;\n    background-color: #fff; \n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}