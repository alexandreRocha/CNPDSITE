ow: hidden;\n  margin: -30px -30px 15px -30px;\n  position: relative;\n  height: 500px;\n}\n\nimg {\n  border-radius: 10px;\n}\n#descricao {\n  text-align: justify;\n  padding-left: 50px;\n}\n.date {\n  font-size: 14px;\n  color: #013365;\n}\n</style>\n"],"mappings":"AAkEA,SAASA,YAAW,QAAS,YAAY;AAEzC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CAAC,IAAI,CAAC;EAEbC,IAAI,EAAE,SAAAA,CAAA,EAAY;IAChB,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAU,GAAI,uBAAuB,EAAE;IAC5CC,KAAK,CAAE,GAAEP,YAAa,gBAAe,GAAE,IAAI,CAACQ,MAAM,CAACC,MAAM,CAACC,EAAE,EACzDC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAER,IAAI,IAAM,IAAI,CAACC,UAAS,GAAID,IAAK,EACvCW,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,OAAO,CAAC,CAAC;EAC7C;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}