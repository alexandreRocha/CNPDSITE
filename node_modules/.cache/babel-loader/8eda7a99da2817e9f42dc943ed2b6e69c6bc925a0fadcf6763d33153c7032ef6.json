io de Atividade Nº.{{ lista.num_doc }} - {{ lista.titulo }}</b\n            >s\n          </router-link>\n        </h5>\n      </div>\n      <div v-if=\"lista\">\n        <div class=\"section-title\">\n          <h2>Relatório Nº.{{ lista.num_doc }} - {{ lista.titulo }} </h2>\n        </div>\n        <p></p> \n        {{ `${API_ENDPOINT}/storage/publicacoesPdf/${lista.anexo}` }}\n        <div class=\"post-img\"> \n          <embed class=\"borda\" :src=\"`${API_ENDPOINT}/storage/publicacoesPdf/${lista.anexo}`\" width=\"100%\" height=\"800px\" />\n\n         </div>\n      </div>\n    </div>\n  </section>\n  <!-- End relatorio Section -->\n</template>\n\n<script>\n\nimport { API_ENDPOINT } from \"../main.js\";\n\nexport default { \n  name: \"RelatorioId\",\n  props: [\"id\"],\n\n  data: function () {\n    return {\n      lista: null,\n    };\n   \n  }, \n\n  created() {\n    console.log(API_ENDPOINT); // Isso deve imprimir o valor no console\n  },\n  mounted() { \n    fetch(`${API_ENDPOINT}/api/relatorio/`+ this.$route.params.id)\n      .then((response) => response.json())\n      .then((data) => (this.lista = data))\n      .catch((err) => console.log(err.message));\n  },\n};\n</script>\n\n<style scoped>\n.relatorio {\n  padding-top: 100px;\n}\n\n.section-title {\n  text-align: center;\n  padding-bottom: 30px;\n}\n.section-title h2 {\n  font-size: 13px;\n  letter-spacing: 1px;\n  font-weight: 700;\n  padding: 8px 20px;\n  line-height: 1;\n  margin: 0;\n  background: #bd9a13;\n  color: #000;\n  display: inline-block;\n  text-transform: uppercase;\n  border-radius: 50px;\n}\n@media (min-width: 1024px) {\n  .section-title p {\n    width: 50%;\n  }\n}\n\n.borda {\n  border-radius: 25px;\n}\npost-img {\n  overflow: hidden;\n  margin: -30px -30px 15px -30px;\n  position: relative;\n  border: 1px solid #061536;\n  border-radius: 25px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}