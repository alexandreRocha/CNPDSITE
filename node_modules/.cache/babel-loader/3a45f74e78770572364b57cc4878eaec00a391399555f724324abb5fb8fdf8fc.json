a\" class=\"queixa\">\n    <div class=\"container\">\n      <div class=\"section-title\">\n        <h2>FORMULÁRIO DE APRESENTAÇÃO DE QUEIXAS/RECLAMAÇÕES</h2>\n      </div>\n      <div class=\"col-lg-12 mt-5 mt-lg-0\">\n        <form\n          action=\"forms/queixa.php\"\n          method=\"post\"\n          role=\"form\"\n          class=\"php-email-form\"\n        >\n          <div class=\"row\">\n            <div class=\"col-lg-6 mt-5 mt-lg-0\">\n             \n              <p class=\"pleft\"> Dados do queixoso:</p>\n               \n              <div class=\"col-md-12 form-group\">\n                <input\n                  type=\"text\"\n                  name=\"name\"\n                  class=\"form-control\"\n                  id=\"name\"\n                  placeholder=\"Seu Nome\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group\">\n                <input\n                  type=\"text\"\n                  name=\"address\"\n                  class=\"form-control\"\n                  id=\"address\"\n                  placeholder=\"Sua morada ...\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group mt-3 mt-md-0\">\n                <input\n                  type=\"phone\"\n                  class=\"form-control\"\n                  name=\"phone\"\n                  id=\"email\"\n                  placeholder=\"Nº de telefone Ex: 5900000\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group mt-3 mt-md-0\">\n                <input\n                  type=\"email\"\n                  class=\"form-control\"\n                  name=\"email\"\n                  id=\"email\"\n                  placeholder=\"Entre seu email Ex: cnpd@cnpd.cv\"\n                  required\n                />\n              </div>\n            </div>\n\n            <div class=\"col-lg-6 mt-5 mt-lg-0\">\n              <p class=\"pleft\"> Dados do denunciado:</p>\n              <div class=\"col-md-12 form-group\">\n                <input\n                  type=\"text\"\n                  name=\"entidade\"\n                  class=\"form-control\"\n                  id=\"entidade\"\n                  placeholder=\"Queixa contra a Pessoa/Empresa/Entidade ... Ex: Empresa XPTO\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group\">\n                <input\n                  type=\"text\"\n                  name=\"entidadeaddress\"\n                  class=\"form-control\"\n                  id=\"entidadeaddress\"\n                  placeholder=\"Morada da Pessoa/Empresa/Entidade... Ex: Palmarejo\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group mt-3 mt-md-0\">\n                <input\n                  type=\"entidadephone\"\n                  class=\"form-control\"\n                  name=\"entidadephone\"\n                  id=\"email\"\n                  placeholder=\"Nº de telefone da Pessoa/Empresa/Entidade... Ex: 5900000\"\n                  required\n                />\n              </div>\n              <div class=\"col-md-12 form-group mt-3 mt-md-0\">\n                <input\n                  type=\"entidadeemail\"\n                  class=\"form-control\"\n                  name=\"entidadeemail\"\n                  id=\"email\"\n                  placeholder=\"Entre o email da Pessoa/Empresa/Entidade...  Ex: xpto@xpto.cv\"\n                  required\n                />\n              </div>\n            </div>\n            \n            <p class=\"pleft\"> Obejeto da sua queixa:</p>\n            <div class=\"col-md-12 form-group\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                name=\"subject\"\n                id=\"subject\"\n             