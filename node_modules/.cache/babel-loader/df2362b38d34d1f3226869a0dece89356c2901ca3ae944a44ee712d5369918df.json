{"ast":null,"code":"import { API_ENDPOINT } from \"../main.js\";\nexport default {\n  name: \"RelatorioId\",\n  props: [\"id\"],\n  data() {\n    return {\n      lista: null,\n      // Declarando a variável xpto\n      xpto: \"\"\n    };\n  },\n  created() {\n    console.log(API_ENDPOINT);\n    // Atribuindo o valor de xpto\n    this.xpto = `${API_ENDPOINT}/storage/publicacoesPdf/${this.lista.anexo}`;\n  },\n  mounted() {\n    fetch(`${API_ENDPOINT}/api/relatorio/` + this.$route.params.id).then(response => response.json()).then(data => {\n      this.lista = data;\n      // Atualizando o valor de xpto quando a lista é carregada\n      this.xpto = `${API_ENDPOINT}/storage/publicacoesPdf/${this.lista.anexo}`;\n    }).catch(err => console.log(err.message));\n  }\n};","map":{"version":3,"mappings":"AAiBA,SAASA,YAAW,QAAS,YAAY;AAEzC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE,CAAC,IAAI,CAAC;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACX;MACAC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAG,CAACR,YAAY,CAAC;IACzB;IACA,IAAI,CAACK,IAAG,GAAK,GAAEL,YAAa,2BAA0B,IAAI,CAACI,KAAK,CAACK,KAAM,EAAC;EAC1E,CAAC;EACDC,OAAO,GAAG;IACRC,KAAK,CAAE,GAAEX,YAAa,iBAAe,GAAI,IAAI,CAACY,MAAM,CAACC,MAAM,CAACC,EAAE,EAC3DC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEZ,IAAI,IAAK;MACd,IAAI,CAACC,KAAI,GAAID,IAAI;MACjB;MACA,IAAI,CAACE,IAAG,GAAK,GAAEL,YAAa,2BAA0B,IAAI,CAACI,KAAK,CAACK,KAAM,EAAC;IAC1E,CAAC,EACAS,KAAK,CAAEC,GAAG,IAAKZ,OAAO,CAACC,GAAG,CAACW,GAAG,CAACC,OAAO,CAAC,CAAC;EAC7C;AACF,CAAC","names":["API_ENDPOINT","name","props","data","lista","xpto","created","console","log","anexo","mounted","fetch","$route","params","id","then","response","json","catch","err","message"],"sourceRoot":"","sources":["/Users/jailsoncorreia/CNPDSYSTEM/SITECNPDFINAL/src/components/RelatorioId.vue"],"sourcesContent":["<template>\n  <!-- ... (seu código existente) ... -->\n  <div v-if=\"lista\">\n    <div class=\"section-title\">\n      <h2>Relatório Nº.{{ lista.num_doc }} - {{ lista.titulo }} </h2>\n    </div>\n    <p></p>\n    {{ `${API_ENDPOINT}/storage/publicacoesPdf/${lista.anexo}` }}\n    <div class=\"post-img\">\n      <!-- Criando a variável xpto -->\n      <embed class=\"borda\" :src=\"xpto\" width=\"100%\" height=\"800px\" />\n    </div>\n  </div>\n  <!-- ... (seu código existente) ... -->\n</template>\n\n<script>\nimport { API_ENDPOINT } from \"../main.js\";\n\nexport default {\n  name: \"RelatorioId\",\n  props: [\"id\"],\n  data() {\n    return {\n      lista: null,\n      // Declarando a variável xpto\n      xpto: \"\",\n    };\n  },\n  created() {\n    console.log(API_ENDPOINT);\n    // Atribuindo o valor de xpto\n    this.xpto = `${API_ENDPOINT}/storage/publicacoesPdf/${this.lista.anexo}`;\n  },\n  mounted() {\n    fetch(`${API_ENDPOINT}/api/relatorio/` + this.$route.params.id)\n      .then((response) => response.json())\n      .then((data) => {\n        this.lista = data;\n        // Atualizando o valor de xpto quando a lista é carregada\n        this.xpto = `${API_ENDPOINT}/storage/publicacoesPdf/${this.lista.anexo}`;\n      })\n      .catch((err) => console.log(err.message));\n  },\n};\n</script>\n\n\n<style scoped>\n.relatorio {\n  padding-top: 100px;\n}\n\n.section-title {\n  text-align: center;\n  padding-bottom: 30px;\n}\n.section-title h2 {\n  font-size: 13px;\n  letter-spacing: 1px;\n  font-weight: 700;\n  padding: 8px 20px;\n  line-height: 1;\n  margin: 0;\n  background: #bd9a13;\n  color: #000;\n  display: inline-block;\n  text-transform: uppercase;\n  border-radius: 50px;\n}\n@media (min-width: 1024px) {\n  .section-title p {\n    width: 50%;\n  }\n}\n\n.borda {\n  border-radius: 25px;\n}\npost-img {\n  overflow: hidden;\n  margin: -30px -30px 15px -30px;\n  position: relative;\n  border: 1px solid #061536;\n  border-radius: 25px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}